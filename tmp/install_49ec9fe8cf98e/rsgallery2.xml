<?xml version="1.0" ?>
<install type="component" version="1.5.0">

  <name>RSGallery2</name>
  <creationDate>2008</creationDate>
  <author>rsgallery2.net</author>
  <copyright>This component is released under the GNU/GPL License</copyright>
  <authorEmail>info@rsgallery2.net</authorEmail>
  <authorUrl>http://rsgallery2.net</authorUrl>
  <version>1.16.0</version>
  <description>RSGallery2 is a full featured gallery component for Joomla!  Visit the official site for FAQ, support forums and latest news: www.RSGallery2.net.</description>

  <files folder="site">
    <folder>flash</folder>
    <folder>lib</folder>
    <folder>templates</folder>

    <filename>rsgallery.css</filename>
    <filename>main.rsgallery2.php</filename>
    <filename>rsgallery2.php</filename>
    <filename>slideshow.rsgallery2.php</filename>
    <filename>picklist.js</filename>
    <filename>router.php</filename>
    <filename>rsgallery2.config.php</filename>
  </files>

  <images folder="site">
    <filename>images/no_pics.gif</filename>
    <filename>images/home.gif</filename>
    <filename>images/folder.gif</filename>
    <filename>images/start.jpg</filename>
    <filename>images/stop.jpg</filename>
    <filename>images/next.jpg</filename>
    <filename>images/previous.jpg</filename>
    <filename>images/edit.png</filename>
    <filename>images/no_edit.png</filename>
    <filename>images/delete.png</filename>
    <filename>images/no_delete.png</filename>
    <filename>images/status_owner.png</filename>
    <filename>images/status_upload.png</filename>
    <filename>images/status_hidden.png</filename>
    <filename>images/shadow.gif</filename>
    <filename>images/shadowAlpha.png</filename>
    <filename>images/search.png</filename>
  </images>

  <installfile>install.rsgallery2.php</installfile>
  <uninstallfile>uninstall.rsgallery2.php</uninstallfile>

  <languages folder="languages">
    <language tag="en-US">en-US.com_rsgallery2.ini</language>
    <language tag="es-ES">es-ES.com_rsgallery2.ini</language>
    <language tag="fr-FR">fr-FR.com_rsgallery2.ini</language>
    <language tag="de-DE">de-DE.com_rsgallery2.ini</language>
    <language tag="it-IT">it-IT.com_rsgallery2.ini</language>
    <language tag="hu-HU">hu-HU.com_rsgallery2.ini</language>
    <language tag="pt-BR">pt-BR.com_rsgallery2.ini</language>
    <language tag="nl-NL">nl-NL.com_rsgallery2.ini</language>
    <language tag="nb-NO">nb-NO.com_rsgallery2.ini</language>
  </languages>

  <administration>
    <menu>RSGallery2</menu>
    <submenu>
      <menu link="option=com_rsgallery2">Control Panel</menu>
      <menu link="option=com_rsgallery2&amp;rsgOption=images&amp;task=batchupload">Batch Upload</menu>
      <menu link="option=com_rsgallery2&amp;rsgOption=images&amp;task=upload">Upload</menu>
      <menu link="option=com_rsgallery2&amp;rsgOption=galleries">Galleries</menu>
      <menu link="option=com_rsgallery2&amp;rsgOption=images">Items</menu>
    </submenu>
    <files folder="admin">
      <folder>help</folder>
      <folder>includes</folder>
      <folder>options</folder>
      <folder>sql</folder>
      <folder>fonts</folder>

      <filename>admin.rsgallery2.css</filename>
      <filename>admin.rsgallery2.html.php</filename>
      <filename>admin.rsgallery2.php</filename>
      <filename>config.rsgallery2.php</filename>
      <filename>rsgallery2.css.php</filename>
      <filename>changelog.php</filename>
      <filename>init.rsgallery2.php</filename>
      <filename>toolbar.rsgallery2.html.php</filename>
      <filename>toolbar.rsgallery2.php</filename>
    </files>

    <images folder="admin">
      <filename>images/rsg2-categories.png</filename>
      <filename>images/rsg2-cpanel.png</filename>
      <filename>images/rsg2-logo.png</filename>
      <filename>images/rsg2-mediamanager.png</filename>
      <filename>images/categories.png</filename>
      <filename>images/config.png</filename>
      <filename>images/cpanel.png</filename>
      <filename>images/dbrestore.png</filename>
      <filename>images/frontpage.png</filename>
      <filename>images/generic.png</filename>
      <filename>images/install.png</filename>
      <filename>images/mediamanager.png</filename>
      <filename>images/menu.png</filename>
      <filename>images/sub_arrow.png</filename>
      <filename>images/upload.png</filename>
      <filename>images/upload_zip.png</filename>
      <filename>images/rsg2_menu.png</filename>
      <filename>images/cssedit.png</filename>
      <filename>images/template.png</filename>
      <filename>images/db_optimize.png</filename>
      <filename>images/maintenance.png</filename>
      <filename>images/blockdevice.png</filename>
    </images>

    <languages folder="languages">
      <language tag="en-US">en-US.com_rsgallery2.ini</language>
      <language tag="es-ES">es-ES.com_rsgallery2.ini</language>
      <language tag="fr-FR">fr-FR.com_rsgallery2.ini</language>
      <language tag="de-DE">de-DE.com_rsgallery2.ini</language>
      <language tag="it-IT">it-IT.com_rsgallery2.ini</language>
      <language tag="hu-HU">hu-HU.com_rsgallery2.ini</language>
      <language tag="pt-BR">pt-BR.com_rsgallery2.ini</language>
      <language tag="nl-NL">nl-NL.com_rsgallery2.ini</language>
      <language tag="nb-NO">nb-NO.com_rsgallery2.ini</language>
    </languages>

  </administration>
</install>